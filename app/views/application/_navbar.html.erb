<%# cache([site, I18n.locale, 'header']) do %>
<!-- Begin Header -->
<header class="navbar navbar-default navbar-fixed-top">
  <%#= render "locales_select" %>
  <div class="container-fluid">
    <div class="navbar-header">
      <%= link_to(raw("<span>Julie</span><span>Gauthron</span>"), root_url, :class => "navbar-brand") %>
      <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".navbar-collapse">
        <i class="icon ion-navicon-round"></i>
      </button>
    </div>
    <nav class="collapse navbar-collapse navbar-right">
      <%= render 'menu', :sections => site.primary_menu, :menu_name => 'navbar-nav' %>
    </nav>
    <%= link_to "https://www.facebook.com/exquiseesquisse72", :id => "facebook", :class => "btn btn-primary navbar-btn", :target => :blank do %>
      <i class="icon ion-social-facebook"></i>
    <% end %>
  </div>
</header>
<%# end %>